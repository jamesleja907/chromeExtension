{
  "manifest_version": 2,
  "name": "API Mocker",
  "version": "1.0.0",
  "description": "Attempt to intercept API calls for easier mocking",
  "background": {
    "scripts": ["app/interceptor.js"]
  },
  "permissions": [
    "webRequest",
    "*://www.google.com/"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open popup"
  },
  "content_security_policy": "default-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='"
}
